{
	"__tokens": {
		"<name>": "(?P<name>[a-zA-Z0-9-_]+)",
		"<ropt>": "(?P<ropt>[a-zA-Z0-9-]+)",
		"<child_nodes>": "(?P<child_nodes>.*)",
		"<append>": ["aaaa", "<append>"]
	},


	"__modifiers": {
		"te.<name>.herit.<child_nodes>": "te.<name>.<child_nodes>"
	},

	"environment.compiler-target": null,
	"environment.runtime-target": null,
	"environment.group-target": null,
	"environment.iterators.<name>": "criterion.<name>.values",
	"environment.naming.<name>": "criterion.<name>.subtitle",
	"environment.cluster.max_nodes" : "machine.partitions.default.nodes",
	"environment.cluster.max_cores_per_node": "machine.partitions.default.cores_per_node",
	"environment.cluster.launcher": "machine.partitions.default.command.alloc",
	"environment.validation.run_wrapper": "machine.partitions.default.wrapper.alloc",
	"environment.validation.compil_wrapper": "machine.partitions.default.wrapper.run",
	"environment.validation.worker_mintime": "machine.partitions.default.batch_mintime",
	"environment.validation.worker_maxtime": "machine.partitions.default.batch_maxtime",
	"environment.validation.workers": "machine.partitions.default.concurrent_batchs",
	"environment.cluster.args": null,
	"environment.validation.timeout": null,
	"environment.validation.expect-success": "settings.fail_on_error",
	"environment.validation.tee": "settings.tee",
	"environment.validation.sched_policy": "settings.scheduling.model",
	"environment.validation.resource_level": "settings.scheduling.sched_on",
	"environment.validation.logging": "settings.result.logs",
	"environment.validation.formats": "settings.result.format", 
	"environment.validation.verbose": "settings.verbose",
	"environment.validation.job_meantime": null,

	"compiler.c" : "compiler.commands.c",
	"compiler.cxx" : "compiler.commands.cxx",
	"compiler.cu" : "compiler.commands.cu",
	"compiler.f77" : "compiler.commands.f77",
	"compiler.f90" : "compiler.commands.f90",
	"compiler.f95" : "compiler.commands.f95",
	"compiler.f03" : "compiler.commands.f03",
	"compiler.f08" : "compiler.commands.f08",
	"compiler.cflags" : null,
	"compiler.openmp" : "compiler.variants.openmp.args",
	"compiler.tbb": "compiler.variants.tbb.args",
	"compiler.accl": "compiler.variants.accl.args",
	"compiler.strict": "compiler.variants.scrict.args",
	
	"runtime.cmd": "runtime.program",
	"runtime.args": "runtime.args",
	"runtime.module": "runtime.plugin",
	"runtime.timeout_prefix": null,
	"runtime.<name>.key": "runtime.iterators.<name>.option",
	"runtime.<name>.usage": "runtime.iterators.<name>.type",
	"runtime.<name>.<ropt>_val": "runtime.iterators.<name>.values.<ropt>",

	"te.<name>.type": null,
	"te.<name>.files": "<name>.build.sources.files",
	"te.<name>.bin": ["<name>.build.sources.binary", "<name>.run.program"],
	"te.<name>.cargs": "<name>.build.cflags",
	"te.<name>.chdir": ["<name>.build.cwd", "<name>.run.cwd"],
	"te.<name>.target": "<name>.build.make.target",
	"te.<name>.openmp": "<name>.build.variants.<append>",
	"te.<name>.tbb": "<name>.build.variants.<append>",
	"te.<name>.accl": "<name>.build.variants.<append>",
	"te.<name>.deps": "<name>.run.depends_on",
	"te.<name>.args": "<name>.run.iterate.program.args.values",
	"te.<name>.time": "<name>.run.validate.time.mean_time",
	"te.<name>.tolerance": "<name>.run.validate.time.tolerance",
	"te.<name>.timeout": "<name>.run.validate.time.kill_after",
	"te.<name>.returns": "<name>.run.validate.expect_exit",
	"te.<name>.herit": null
}
